    "import pandas as pd\n",
    "import numpy as np\n",
    "idx = 0\n",
    "def count_values(ex):\n",
    "    count = df[\"名前\"]==ex\n",
    "    global idx\n",
    "    idx = count.sum()\n",
    "    return idx\n",
    "\n",
    "df = pd.read_excel(\"test.xlsx\")\n",
    "df.loc[:,\"出現回数\"] = df.loc[:,\"名前\"].apply(count_values)\n",
    "\n",
    "print(idx)\n",
    "\n",
    "df"
   ]
